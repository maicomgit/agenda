<h1>Editand Usuario </h1>


<%= form_for @usuario do |f| %>
    <%= f.label :nome, 'Nome' %>
    <%= f.text_field :nome  %>
    <br>
    <br>
    <%= f.label :email, 'Email' %>
    <%= f.text_field :email %>
    <br>
    <br>
    <%= f.label :login, 'Login' %>
    <%= f.text_field :login %>
    <br>
    <br>
    <%= f.label :senha, 'Senha' %>
    <%= f.text_field :senha  %>
    <br>
    <br>
    <%= f.submit 'Salvar' %>

<% end %>
